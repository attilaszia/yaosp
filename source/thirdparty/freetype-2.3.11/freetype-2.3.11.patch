+
+<build default="all">
+    <array name="files">
+        <item>src/autofit/autofit.c</item>
+        <item>src/base/ftbase.c</item>
+        <item>src/base/ftinit.c</item>
+        <item>src/base/ftsystem.c</item>
+        <item>src/base/ftglyph.c</item>
+        <item>src/base/ftbitmap.c</item>
+        <item>src/base/fttype1.c</item>
+        <item>src/truetype/truetype.c</item>
+        <item>src/sfnt/sfnt.c</item>
+        <item>src/smooth/smooth.c</item>
+    </array>
+
+    <target name="clean">
+        <delete>objs/*</delete>
+        <rmdir>objs</rmdir>
+    </target>
+
+    <target name="prepare" type="private">
+        <mkdir>objs</mkdir>
+    </target>
+
+    <target name="compile">
+        <call target="prepare"/>
+
+        <echo>Compiling freetype 2.3.11</echo>
+
+        <for var="i" array="${files}">
+            <echo>-> ${i}</echo>
+            <gcc>
+                <input>${i}</input>
+                <output>objs/filename(${i}).o</output>
+                <include>../../../include</include>
+                <include>../../../../build/crosscompiler/lib/gcc/i686-pc-yaosp/4.3.3/include</include>
+                <include>./include</include>
+                <flag>-c</flag>
+                <flag>-O2</flag>
+                <flag>-m32</flag>
+                <flag>-Wall</flag>
+                <flag>-nostdinc</flag>
+                <flag>-nostdlib</flag>
+                <flag>-fno-builtin</flag>
+                <flag>-fno-strict-aliasing</flag>
+                <define key="FT2_BUILD_LIBRARY"/>
+            </gcc>
+        </for>
+
+        <echo>Linking freetype 2.3.9</echo>
+        <echo>-> objs/libfreetype.a</echo>
+
+        <exec executable="ar">
+            <arg>-rc</arg>
+            <arg>objs/libfreetype.a</arg>
+            <arg>objs/*.o</arg>
+        </exec>
+    </target>
+
+    <target name="install">
+        <echo>Installing freetype 2.3.11 headers</echo>
+
+        <mkdir>../../../include/freetype</mkdir>
+        <mkdir>../../../include/freetype/config</mkdir>
+
+        <copy from="include/ft2build.h" to="../../../include/ft2build.h"/>
+        <copy from="include/freetype/*.h" to="../../../include/freetype"/>
+        <copy from="include/freetype/config/*.h" to="../../../include/freetype/config"/>
+    </target>
+
+    <target name="all">
+        <call target="clean"/>
+        <call target="compile"/>
+        <call target="install"/>
+    </target>
+</build>
