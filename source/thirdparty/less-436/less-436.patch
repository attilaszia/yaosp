+        <item>ifile.c</item>
+        <item>input.c</item>
+        <item>jump.c</item>
+        <item>line.c</item>
+        <item>linenum.c</item>
+        <item>lsystem.c</item>
+        <item>mark.c</item>
+        <item>optfunc.c</item>
+        <item>option.c</item>
+        <item>opttbl.c</item>
+        <item>os.c</item>
+        <item>output.c</item>
+        <item>pattern.c</item>
+        <item>position.c</item>
+        <item>prompt.c</item>
+        <item>search.c</item>
+        <item>signal.c</item>
+        <item>tags.c</item>
+        <item>ttyin.c</item>
+        <item>version.c</item>
+    </array>
+
+    <target name="clean">
+        <delete>objs/*</delete>
+        <rmdir>objs</rmdir>
+    </target>
+
+    <target name="prepare" type="private">
+        <mkdir>objs</mkdir>
+    </target>
+
+    <target name="compile">
+        <call target="prepare"/>
+
+        <echo>Compiling less 436</echo>
+
+        <for var="i" array="${files}">
+            <echo>-> ${i}</echo>
+            <gcc>
+                <input>${i}</input>
+                <output>objs/filename(${i}).o</output>
+                <include>./</include>
+                <flag>-c</flag>
+                <flag>-O2</flag>
+                <flag>-Wall</flag>
+                <flag>-fno-strict-aliasing</flag>
+            </gcc>
+        </for>
+
+        <echo>Linking less 436</echo>
+        <echo>-> objs/less</echo>
+
+        <gcc>
+            <input>objs/*.o</input>
+            <input>../../../../build/libs/libncurses.a</input>
+            <output>objs/less</output>
+        </gcc>
+    </target>
+
+    <target name="install">
+        <echo>Installing less binary</echo>
+
+        <copy from="objs/less" to="../../../../build/image/application/less"/>
+    </target>
+
+    <target name="all">
+        <call target="clean"/>
+        <call target="compile"/>
+        <call target="install"/>
+    </target>
+</build>
