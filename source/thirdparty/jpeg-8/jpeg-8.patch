diff -Nru jpeg-8-orig/pbuild.xml jpeg-8/pbuild.xml
--- jpeg-8-orig/pbuild.xml	1970-01-01 01:00:00.000000000 +0100
+++ jpeg-8/pbuild.xml	2010-02-08 09:32:15.000000000 +0100
@@ -0,0 +1,130 @@
